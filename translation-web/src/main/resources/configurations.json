{
    "detect": {
    	"services": [
            {
            	"id": "PangeanicLangDetectService",
              	"classname": "eu.europeana.api.translation.web.service.PangeanicLangDetectService",
              	"supported": [ "en", "fr", "de" ]
            }
        ],
        "default": "PangeanicLangDetectService"
    },
    "translate": {
	 	"services": [
            { 
                "id": "PangeanicTranslationService",
                "classname": "eu.europeana.api.translation.web.service.PangeanicTranslationService",
                "supported": [ "de-en", "fr-en" ]
            },
            {
            	"id": "GoogleTranslationService",
            	"classname": "eu.europeana.api.translation.web.service.GoogleTranslationService",
            	"supported": [ "de-en", "fr-en" ]
            }                        
        ],
        "default": "PangeanicTranslationService"
    }
}
